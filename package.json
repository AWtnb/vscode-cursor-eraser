{
  "name": "cursor-eraser",
  "publisher": "awtnb",
  "repository": {
    "url": "https://github.com/AWtnb/vscode-cursor-eraser"
  },
  "icon": "images/logo.png",
  "displayName": "cursor-eraser",
  "description": "Erase unnecessary cursors in multi-cursor mode.",
  "version": "0.1.5",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Multi-cursor"
  ],
  "activationEvents": [
    "onCommand:cursor-eraser.erase-toBegin",
    "onCommand:cursor-eraser.erase-toEnd",
    "onCommand:cursor-eraser.keep-match",
    "onCommand:cursor-eraser.erase-match"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-eraser.erase-toBegin",
        "title": "cursor-eraser: erase to BEGIN of file"
      },
      {
        "command": "cursor-eraser.erase-toEnd",
        "title": "cursor-eraser: erase to END of file"
      },
      {
        "command": "cursor-eraser.keep-match",
        "title": "cursor-eraser: KEEP match"
      },
      {
        "command": "cursor-eraser.erase-match",
        "title": "cursor-eraser: ERASE match"
      }
    ],
    "configuration": {
      "title": "cursor-eraser",
      "properties": {
        "cursor-eraser.caseSensitive": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
